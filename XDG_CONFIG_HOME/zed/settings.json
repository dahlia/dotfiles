{
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4"
    },
    "version": "2"
  },
  "theme": {
    "mode": "system",
    "dark": "One Dark",
    "light": "One Light"
  },
  "buffer_font_size": 18,
  "buffer_font_family": "Sarasa Mono K",
  "buffer_font_weight": 500,
  "vim_mode": true,
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "copilot": {
    "enabled": true
  },
  "show_wrap_guides": true,
  "wrap_guides": [80]
}
